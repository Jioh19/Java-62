package com.edutecno;

public class Resumen {

	public static void main(String[] args) {
		//Crear arreglos!
		
		int[] arregloInt = {1, 2, 3}; //Con inicializacion
		String arregloString[]; // Arreglo declarado de la segunda forma sin inicializacion
		int[] arregloInt3 = new int[5]; //Arreglo declardo e inicializado de tamaño 5
		arregloInt2 = new String[2]; // Arreglo previamente declarado, ahora inicializade de tamaño 2
		
		
	}

}
